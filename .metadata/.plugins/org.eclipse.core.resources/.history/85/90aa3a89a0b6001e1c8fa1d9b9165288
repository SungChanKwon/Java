package ex03;

import java.util.Scanner;

public class Ifexam {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner s = new Scanner(System.in);
		int age = s.nextInt();
		
		if (age >= 20) {
			System.out.println("성년입니다");
		}
		if (age < 20) {
			System.out.println("미성년입니다");
		}
		
	}

}
