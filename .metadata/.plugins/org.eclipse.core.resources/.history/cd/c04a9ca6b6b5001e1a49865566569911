package ex01;

public class Hello {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 자바 프로그래밍 언어
		System.out.println("자바 프로그래밍 언어");
		System.out.println("Hello World!!!");
		System.out.println();
		System.out.println();
	
		String myName = "홍길동";
		int myAge = 20;
		
		System.out.println("이름 :\n" + myName);
		System.out.println("나이 :\\ " + myAge);
		
		
		
		
	}

}
